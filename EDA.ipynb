import pandas as pd
import numpy as np
import matplotlib.pyplot as plt
import seaborn as sns

# Load dataset
df = pd.read_csv("StudentsPerformance.csv")   # If notebook is inside /notebooks
# If notebook is in root folder, use this instead:
# df = pd.read_csv("StudentsPerformance.csv")

# Show first few rows
print("First 5 rows of dataset:")
display(df.head())

# Dataset info
print("\nDataset Information:")
display(df.info())

# Summary statistics
print("\nStatistical Summary:")
display(df.describe())

# Check for missing values
print("\nMissing Values in Dataset:")
display(df.isnull().sum())

# Distribution plots
plt.figure(figsize=(6,4))
sns.histplot(df["math score"], kde=True, color='blue')
plt.title("Distribution of Math Score")
plt.show()

plt.figure(figsize=(6,4))
sns.histplot(df["reading score"], kde=True, color='green')
plt.title("Distribution of Reading Score")
plt.show()

plt.figure(figsize=(6,4))
sns.histplot(df["writing score"], kde=True, color='purple')
plt.title("Distribution of Writing Score")
plt.show()

# Boxplot comparison
plt.figure(figsize=(7,5))
sns.boxplot(data=df[["math score","reading score","writing score"]])
plt.title("Score Comparison Boxplot")
plt.show()

# Pairplot for relationships
sns.pairplot(df[["math score","reading score","writing score"]])
plt.show()

# Correlation heatmap
plt.figure(figsize=(10,6))
sns.heatmap(df.corr(), annot=True, cmap="coolwarm")
plt.title("Correlation Heatmap")
plt.show()

# Gender vs score analysis
plt.figure(figsize=(6,4))
sns.barplot(x="gender", y="math score", data=df)
plt.title("Math Score by Gender")
plt.show()

# Parental education vs score
plt.figure(figsize=(10,5))
sns.countplot(data=df, x="parental level of education")
plt.xticks(rotation=45)
plt.title("Parental Education Distribution")
plt.show()

plt.figure(figsize=(6,4))
sns.boxplot(x="lunch", y="math score", data=df)
plt.title("Lunch Type vs Math Score")
plt.show()

# Final insights summary
print("Mean Math Score:", df["math score"].mean())
print("Highest Math Score:", df["math score"].max())
print("Lowest Math Score:", df["math score"].min())

